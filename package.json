{
  "name": "dsc.eco",
  "version": "2.0.5",
  "description": "A quick way to implement economy features to your discord bot",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "typedoc": "typedoc --theme minimal --out docs src/",
    "pretty": "prettier --config .prettierrc \"./src/**/*.ts\"  \"./lib/**/*.js\" --write",
    "build": "npm run tsc && npm run pretty && npm run typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurgenjacobsen/dsc.eco.git"
  },
  "keywords": [
    "economy",
    "discord",
    "eco",
    "dsc"
  ],
  "author": "jurgenjacobsen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jurgenjacobsen/dsc.eco/issues"
  },
  "homepage": "https://github.com/jurgenjacobsen/dsc.eco#readme",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "discord.js": "^13.1.0",
    "dsc.db": "^3.0.4",
    "events": "^3.3.0"
  }
}
